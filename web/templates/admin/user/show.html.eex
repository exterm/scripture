<h2>Show user</h2>

<ul>
  <li>
    <strong>Name:</strong>
    <%= full_name(@user) %>
  </li>
  <li>
    <strong>E-Mail Address:</strong>
    <%= @user.email %>
  </li>
  <li>
    <strong>Role:</strong>
    <%= @user.role %>
  </li>
</ul>

<%= link "Edit", to: admin_user_path(@conn, :edit, @user) %>
<%= link "Back", to: admin_user_path(@conn, :index) %>
